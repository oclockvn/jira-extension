{
  "manifest_version": 3,
  "name": "Blank Chrome Extension",
  "version": "1.0.0",
  "description": "A blank Chrome extension using Manifest V3",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [],
  "host_permissions": [],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.atlassian.net/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
} 